{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_DATABSE_CONNECTION_STRING "
  },
  "JwtConfig": {
    "Secret": "YOUR_SECRET_KEY", 
    "ExpiryTimeFrame": "00:01:00"
  },
  "EmailConfig": {
    "API_KEY" : "MAIL_CLIENT_API_KEY"
  }
}